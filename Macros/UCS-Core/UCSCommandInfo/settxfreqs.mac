procedure(settxfreqs, f1, f2)

   if(gSeq->psInfo->skipCmds == 0)

      args = getargnames()
      cnt = gSeq->psInfo->loopCnt
      gSeq->psInfo->durationNStk[cnt] = gSeq->psInfo->durationNStk[cnt] + 0.5
      gSeq->psInfo->command = gSeq->psInfo->command + ["settxfreqs,$f1$,$f2$,$args[0]$,$args[1]$"]

   endif

endproc()

