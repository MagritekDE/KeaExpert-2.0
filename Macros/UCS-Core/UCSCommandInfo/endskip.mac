procedure(endskip, skipName)

   cnt = gSeq->psInfo->loopCnt
   gSeq->psInfo->durationNStk[cnt] = gSeq->psInfo->durationNStk[cnt]
   gSeq->psInfo->skipCmds = 0
   gSeq->psInfo->command = gSeq->psInfo->command + ["endskip,$skipName$"]

endproc()

