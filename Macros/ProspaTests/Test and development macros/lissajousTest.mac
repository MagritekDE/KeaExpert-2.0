procedure(gx)

   time(0)
   for(k = 1 to 10 step 0.01)
       x = [0:0.001:1]*2*pi
       y = sin(k*x)
       q = cos((k+3)*x)
       draw1d("false")
       plot(y,q)
       grid("finexgrid","true")
       grid("fineygrid","true")
       grid("xgrid","true")
       grid("ygrid","true")
       draw1d("true")

   next(k)
   print(time())
   
endproc()