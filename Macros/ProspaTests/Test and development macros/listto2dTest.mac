procedure(listto2dTest)

   print("\n2D list")
   lst = ["1";"2","3";"4","5","6"]
   :printList(lst)
   print("\n1D list")
   lst = ["1","2","3","4","5","6"]
   :printList(lst)

endproc()

procedure(printList,lst)

   if(vartype(lst) == "list")
      lst = listto2d(lst)
   endif
   (w,h) = size(lst)
   for(y = 0 to h-1)
      for(x = 0 to w[y]-1)
         pr "\nRow = $y$, Col = $x$, Value = $lst[x,y]$"
      next(x)
   next(y)

endproc()

