procedure(toolbarktest)

   n = window("Toolbar test", -1, -1, 300, 74)

       button(1, 220, 5, 50, 32, "Close",closewindow(0))
       toolbar(2,"toolbarktest",
                 "load",pr ("load\n"), 
                 "save",pr ("save\n"), 
                 "print",pr ("print\n"))

       setwindowpar(n,"toolbar",2) 

   showwindow(n,"recalc")

endproc()

