procedure(testfib)

   cd(getmacropath())
   cachemacro(getmacroname())
   cacheproc("true")

   time(0)
   
   for( w = 0 to 1000)
     r = :fibonacci(10)  
   next(w)
   
   pr time()
   pr r

endproc()

procedure(fibonacci,n) 

   n2 = 0
   n1 = 1
   
   for (q = n to 3 step -1)
   
      tmp = n2
      n2 = n1
      n1 = n1 + tmp
   
   next(q)
     

endproc(n1+n2)