procedure(min_test1,verbose,delay)

   x = [0:1000]/1000*pi
   m = -sin(x)
   (v,p) = min(m)

   if(v != -1 | p != 500)
     return(0, "Invalid result for: min_test1 #1")
   endif  

   m = (sin(-x))'*sin(x)
   (v,px,py) = min(m)
   if(v != -1 | px != 500 | py != 500)
     return(0, "Invalid result for: min_test1 #2")
   endif 

endproc(1,"")
