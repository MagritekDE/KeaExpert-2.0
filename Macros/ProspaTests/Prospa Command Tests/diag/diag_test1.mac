procedure(diag_test1,verbose,delay)

   tests = load("diag1.lst")

   for(n = 0 to size(tests)-1)
      s = tests[n]
      if(s[0] != "#")
         if(eval(tests[n]) == 0)
            return(0, "Invalid result for: $tests[n]$")
         endif 

         if(verbose)
            pr "      Valid test: $tests[n]$\n"  
         endif
      endif
   next(n)

endproc(1,"")
