

procedure(rmfile_test1,verbose,delay)

   v = noise(1000)
   save("test.1d",v)

   if(not(isfile("test.1d")))
     return(0, "Invalid result for: rmfile_test1 #1")
   endif 

   rmfile("test.1d")
   if(isfile("test.1d"))
     return(0, "Invalid result for: rmfile_test1 #2")
   endif 

endproc(1,"")

