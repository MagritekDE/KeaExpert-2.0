procedure(identity_test1,verbose,delay)

    n = 9
    m = identity(n+1)
    for(y = 0 to n)
       for(x = 0 to n)
          if(x == y & m[x,y] != 1)
            return(0, "Invalid result for: identity #1")
          endif 
          if(x != y & m[x,y] != 0)
            return(0, "Invalid result for: identity #2")
          endif 
      next(x)
   next(y)

endproc(1,"")