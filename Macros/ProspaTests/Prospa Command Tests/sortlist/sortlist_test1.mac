procedure(sortlist_test1,verbose,delay)

   t = "abcdefghijklmnopqrstuvwxyz"

   lst1 = list(26)
   lst2 = list(26)
   lst3 = list(26)
   lst4 = list(26)
   for(k = 0 to 25)
      lst1[k] = t[k]
      lst2[k] = t[25-k]
      lst3[k] = "a"+t[25-k]
      lst4[k] = "a"+t[k]
   next(k)

   if(sortlist(lst2) != lst1)
     return(0, "Invalid result for: sortlist_test1 #1")
   endif 

   if(sortlist(lst3) != lst4)
     return(0, "Invalid result for: sortlist_test1 #2")
   endif 

endproc(1,"")
