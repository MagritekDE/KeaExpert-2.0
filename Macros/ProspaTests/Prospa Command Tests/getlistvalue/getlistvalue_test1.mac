procedure(getlistvalue_test1,verbose,delay)

# Make a parameter list
   lst = list(100)
   for(k = 0 to 99)
      lst[k] = "a$k$ = $k*2$"
   next(k)

# Check the value of each parameter
   for(k = 0 to 99)
      if(getlistvalue(lst,"a$k$") != "$k*2$")
         return(0, "Invalid result for: getlistvalue_test1() #1")
      endif
  next(k)   
 
endproc(1,"")