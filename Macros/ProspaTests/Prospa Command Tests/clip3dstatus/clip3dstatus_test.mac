procedure(clip3dstatus_test1,verbose,delay)
  
clear3d()
color3d([1,1,0])
clip3dstatus("on")
cylinder([0,-10,0],[0,10,0],4,[1,1,0])
color3d([1,0,0])
box3d(-15,15,-15,15,-15,15)
clip3d(1,0,"xy","plus")
