procedure(insertinlist_test1,verbose,delay)

   N = 5
   a = list(N)
   for(k = 0 to N)
      b = insertinlist(a,"NEW TEXT",k)
      c = 0
      for(m = 0 to N)
         if(m == k & b[m] != "NEW TEXT")
            return(0, "Invalid result for: insertinlist #1")
         endif
         if(m != k)
            if(b[m] != "item $c$")
               return(0, "Invalid result for: insertinlist #1")
            else
               c = c + 1
            endif
         endif
      next(m)
   next (k)

endproc(1,"")



