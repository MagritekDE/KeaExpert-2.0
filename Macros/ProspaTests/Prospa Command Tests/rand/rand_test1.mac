procedure(rand_test1,verbose,delay)

   clear1d()

   n = matrix(10000)
   for(k = 0 to 9999) 
     n[k] = rand(9)+10
   next(k)

   h = matrix(30)

   for(k = 0 to 9999) 
     ind = trunc(n[k])
     h[ind] = h[ind] + 1
   next(k)

   plot(h)
   title("Random number distribution 10-20 10k samples")
   ylabel("Number of values")
   xlabel("Bin")
   pause(delay)

endproc(1,"")
 
