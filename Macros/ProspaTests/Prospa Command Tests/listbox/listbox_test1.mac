procedure(listbox_test1,verbose,delay)

   n = :windowdefinition()
   showwindow(n)
   lst = list(8)
   setpar(n,1,"list",lst)
   txt = getpar(n,1,"list")
   if(txt != lst)
      return(0, "Invalid result for listbox_test")
    endif
   pause(delay)
   closewindow(n)

endproc(1,"")

