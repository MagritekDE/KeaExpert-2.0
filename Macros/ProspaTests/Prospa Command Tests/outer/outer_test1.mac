procedure(outer_test1,verbose,delay)

   v1 = [1,2,3]
   v2 = [1,2,3]

   if(outer(v1,v2') != [1,2,3;2,4,6;3,6,9])
      return(0, "Invalid result for: outer_test1 #1")
   endif  

   v1 = [1,2;3,4]
   v2 = [1,1;1,1]

   if(outer(v1,v2) != [1,1,2,2;1,1,2,2;3,3,4,4;3,3,4,4])
      return(0, "Invalid result for: outer_test1 #2")
   endif  
 
     
endproc(1,"")
 
