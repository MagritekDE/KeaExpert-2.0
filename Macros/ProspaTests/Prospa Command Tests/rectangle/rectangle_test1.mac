# Draw a series of rectangles 
# in the 2D window

procedure(rectangle_test1,verbose,delay)

   m = matrix(512,512)
   
   for k = 0 to 50
     x = rand(500)
     y = rand(500)
     a = rand(100)
     w = rand(100)
     h = rand(100)
     m = rectangle(m,x,y,w,h,a)
     draw2d("false")
     image(m)
     title("rectangle test")
     draw2d("true")
   next(k)

endproc(1,"")
