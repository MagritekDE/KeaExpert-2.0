procedure(getplotstate_test1,verbose,delay)

  curplot("1d")
  txt1 = "my 1D plot"
  setplotstate(txt1)

  curplot("2d")
  txt2 = "my 2D plot"
  setplotstate(txt2)

  curplot("1d")
  if(getplotstate() != txt1)
      return(0, "Invalid result for: getplotstate() 1D")
  endif

  curplot("2d")
  if(getplotstate() != txt2)
      return(0, "Invalid result for: getplotstate() 2D")
  endif

endproc(1,"")