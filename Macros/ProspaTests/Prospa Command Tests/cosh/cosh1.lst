# scalars
cosh(0) = 1
cosh(1) = 1/2*(exp(-1)+exp(1))
cosh(2) = 1/2*(exp(-2)+exp(2))
cosh(10) = 1/2*(exp(-10)+exp(10))
# 1D matrix
cosh([0,1,2,10]) = [1/2*(exp(-0)+exp(0)),1/2*(exp(-1)+exp(1)),1/2*(exp(-2)+exp(2)),1/2*(exp(-10)+exp(10))]
# 2D matrix
cosh([0,1;2,10]) = [1/2*(exp(-0)+exp(0)),1/2*(exp(-1)+exp(1));1/2*(exp(-2)+exp(2)),1/2*(exp(-10)+exp(10))]
