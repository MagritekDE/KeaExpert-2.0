procedure(mergelists_test1,verbose,delay)

   l1 = ["a = 23", "b = 45"]
   l2 = ["b = 45", "e = 23", "a = 18", "f = 45", "q = 123"]

   ltot = sortlist(mergelists(l1,l2))

   if(ltot != ["a = 23", "b = 45", "e = 23", "f = 45", "q = 123"])
     return(0, "Invalid result for: mergelists_test1")
   endif 

endproc(1,"")
