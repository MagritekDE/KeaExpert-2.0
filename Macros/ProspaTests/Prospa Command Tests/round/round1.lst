# real scalars
round(0) = 0
round(1.1) = 1
round(1.45) = 1
round(1.5) = 2
round(1.95) = 2
round(-1.1) = -1
round(-1.45) = -1
round(-1.5) = -2
round(-1.95) = -2
# complex scalars
round(1.1+1.1i) = 1+i
round(1.45+1.45i) = 1+i
round(1.5+1.5i) = 2+2i
round(1.95+1.95i) = 2+2i
round(-1.1-1.1i) = -1-1i
round(-1.45-1.45i) = -1-1i
round(-1.5-1.5i) = -2-2i
round(-1.95-1.95i) = -2-2i
# real 1D matrix
round([0,1.1,1.45,1.5,1.95]) = [0,1,1,2,2]
round([-1.1,-1.45,-1.5,-1.95]) = [-1,-1,-2,-2]
# real 2D matrix
round([1.1,1.45;1.5,1.95]) = [1,1;2,2]
round([-1.1,-1.45;-1.5,-1.95]) = [-1,-1;-2,-2]
# complex 1D matrix
round({1.1+1.1i,1.45+1.45i,1.5+1.5i,1.95+1.95i}) = {1+i,1+i,2+2i,2+2i}
round({-1.1-1.1i,-1.45-1.45i,-1.5-1.5i,-1.95-1.95i}) = {-1-i,-1-i,-2-2i,-2-2i}
# complex 2D matrix
round({1.1+1.1i,1.45+1.45i;1.5+1.5i,1.95+1.95i}) = {1+i,1+i;2+2i,2+2i}
round({-1.1-1.1i,-1.45-1.45i;-1.5-1.5i,-1.95-1.95i}) = {-1-i,-1-i;-2-2i,-2-2i}
