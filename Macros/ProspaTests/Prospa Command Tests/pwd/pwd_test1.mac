procedure(pwd_test1,verbose,delay)

   bak = getcwd()
   cd(getmacropath())

   dir = pwd()
   if(dir != getcwd())
      return(0, "Invalid result for: pwd #1")
   endif 

   mkdir("test1")
   cd("test1")
   dir2 = pwd()
   if(dir2 != dir + "\\test1")
      return(0, "Invalid result for: pwd #2")
   endif 
   
   cd("..")
   rmdir("test1")
   cd("bak")

endproc(1,"")

 
