# findobj function test
procedure(findobj_test1,verbose,delay)

   n = :windowdefinition()
   showwindow(n)
   pause(1)
   on = findobj(n,"name","on")
   setpar(0,on,"label","name = 'on'")
   pause(1)
   off = findobj(n,"name","off")
   setpar(0,off,"label","name = 'off'")
   pause(1)
   closewindow(n)

endproc(1,"")

procedure(windowdefinition)

   n = window("findobj test", 100, 100, 120, 140)
       button(1, 20, 20, 70, 24, "button 1")
       button(2, 20, 60, 70, 24, "button 2")
       setpar(0,1,"name","on")
       setpar(0,2,"name","off")

endproc(n)
