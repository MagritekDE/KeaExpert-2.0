procedure(trac_test1,verbose,delay)
 
# Real 2D matrix trace
   A = [1,2;3,4]
   if(trac(A) != 5)
      return(0, "Invalid result for: trac_test1 #1")
   endif 

# Real 2D matrix trace
   A = [1,2,3;4,5,6;7,8,9]
   if(trac(A) != 15)
      return(0, "Invalid result for: trac_test1 #2")
   endif   

# Complex 2D matrix trace
   A = {1+i,2+2i;3+3i,4+4i}
   if(trac(A) != 5+5i)
      return(0, "Invalid result for: trac_test1 #3")
   endif 

# Complex 2D matrix trace
   A = {1+i,2+2i,3+3i;4+4i,5+5i,6+6i;7+7i,8+8i,9+9i}
   if(trac(A) != 15+15i)
      return(0, "Invalid result for: trac_test1 #4")
   endif 
  
endproc(1,"")
