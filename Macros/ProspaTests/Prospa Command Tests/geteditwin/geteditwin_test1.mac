procedure(geteditwin_test1,verbose,delay)

   n = :windowdefinition()
   showwindow(n)
   pause(delay)
   seteditwin(n)
   if(geteditwin() != n)
      return(0, "Invalid result for: geteditwin_test1()")
   endif
   pause(delay)
   closewindow(n)

endproc(1,"")

procedure(windowdefinition)

   n = window("geteditwin test", 100, 100, 120, 140)
       button(1, 20, 20, 70, 24, "button")

endproc(n)