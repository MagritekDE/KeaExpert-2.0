procedure(ylabelright_test1,verbose,delay)
 
   clear1d()
   
   # Set with no traces, 1D
   ylabelright("I should be invisible")
   
   m = noise(1000)
   m = [0:1:100]^2
   plot(m)
   my_trace = curtrace()
   my_trace->axis("right")
   title("ylabelright test")
   ylabelright("Set right y label")
   pause(1)

   # Set with a trace on the right hand axis and none on the right
   
   my_trace = curtrace()
   my_trace->axis("left")
   ylabelright("I should be invisible")
   
   clear2d()
   
   # Set with no traces, 2D
   ylabelright("Nothing to see here")
   
   m = noise(100,100)
   image(m)
   m = [-100:100]
   m = (m'*m)^2
   image(m)
   title("Title test")
   ylabelright("Nothing to see here")
   pause(1)
   
endproc(1,"")
