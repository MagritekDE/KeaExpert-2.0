# scalars
asin(-1) = -pi/2
asin(-1/sqrt(2)) = -pi/4
asin(0) = 0
asin(1/sqrt(2)) = pi/4
asin(1) = pi/2
asin(2) = nan
asin(-2) = nan
# 1D matrix
asin([-1,-1/sqrt(2),0,1/sqrt(2),1]) = [-pi/2,-pi/4,0,pi/4,pi/2]
asin([-2,2]) = [nan,nan]
# 2D matrix
asin([-1,-1/sqrt(2);0,1/sqrt(2)]) = [-pi/2,-pi/4;0,pi/4]
