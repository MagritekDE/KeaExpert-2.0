procedure(trans_test1,verbose,delay)
 
# Real 1D matrix transposition
   A = [1,2,3,4,5,6]
   if(trans(A) != [1;2;3;4;5;6])
      return(0, "Invalid result for: trans_test1 #1")
   endif   

# Real 2D matrix transposition
   A = [1,2,3;4,5,6;7,8,9]
   if(trans(A) != [1,4,7;2,5,8;3,6,9])
      return(0, "Invalid result for: trans_test1 #2")
   endif 

# Complex 1D matrix transposition
   A = {1+1i,2+2i,3+3i,4+4i,5+5i,6+6i}
   if(trans(A) != {1+1i;2+2i;3+3i;4+4i;5+5i;6+6i})
      return(0, "Invalid result for: trans_test1 #3")
   endif   

# Complex 2D matrix transposition
   A = {1+1i,2+2i,3+3i;4+4i,5+5i,6+6i;7+7i,8+8i,9+9i}
   if(trans(A) != {1+1i,4+4i,7+7i;2+2i,5+5i,8+8i;3+3i,6+6i,9+9i})
      return(0, "Invalid result for: trans_test1 #4")
   endif 

endproc(1,"")
