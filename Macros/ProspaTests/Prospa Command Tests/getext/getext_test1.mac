procedure(getext_test1,verbose,delay)

   if(getext("myfile.asm") != "asm")
        return(0, "Invalid result for: getext_test1() #1")
   endif

   if(getext("myfile") != "")
        return(0, "Invalid result for: getext_test1() #2")
   endif

   if(getext("myfile.longextension") != "longextension")
        return(0, "Invalid result for: getext_test1() #3")
   endif

endproc(1,"")
