# Draw a series of circles 
# in the 2D window

procedure(circle_test1,verbose,delay)

m = matrix(512,512)

for k = 0 to 100
  x = rand(500)
  y = rand(500)
  r = rand(100)
  a = rand(100)
  m = circle(m,x,y,r,a)
  draw2d("false")
  image(m)
  title("circle test")
  draw2d("true")
next(k)

endproc(1,"")
