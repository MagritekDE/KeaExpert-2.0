procedure(progressbar_test1,verbose,delay)

   n = window("progressbar test",100,100,150,100)
   progressbar(1,20,20,100,20,"horizontal");
   setpar(n,1,"range",[0,100])
   showwindow(n)

   for(m = 0 to 100)
      setpar(n,1,"value",m)
      pause(0.05)
   next(m)
   bell()

   closewindow(n)

endproc(1,"")

 
