procedure(procedure_test1,verbose,delay)

# Single argument, single return test
   x  = [1:1:100]
   s = :sumvec(x)
   if(s != sum(x))
      abort("Invalid result for: procedure")
   endif 

endproc(1,"")


# Sum procedure
procedure(sumvec,u)

   total = 0
   for(n = 0 to size(u)-1)
      total = total + u[n]
   next(n)

endproc(total)


 
