procedure(cmap_test1,verbose,delay)
 
   clear2d
   x = [0:99]
   y = sin(x/5)
   draw2d("false")
   image(y'*y)
   cmap(spectrum)
   showcmap("true")
   title("Colormap: spectrum")
   draw2d("true")
   pause(delay)
   cmap(hot)
   title("Colormap: hot")
   pause(delay)
   cmap(copper)
   title("Colormap: copper")
   pause(delay)

endproc(1,"")
