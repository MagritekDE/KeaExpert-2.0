procedure(plot_test1,verbose,delay)

   clear1d()
   x = exp([0:0.1:3])-1
   y = exp(-x/10)
   plot(x,y)
   pause(delay/2)
   plot(x,y,"tracetype","stairs")
   pause(delay/2)
   plot(x,y,"tracetype","dots")
   pause(delay/2)
   plot(x,y,"tracetype","lines","realstyle",1)
   pause(delay/2)
   plot(x,y,"tracetype","lines","realstyle",2)
   pause(delay/2)
   plot(x,y,"tracetype","lines","realstyle",3)
   pause(delay/2)
   plot(x,y,"tracetype","lines","realstyle",4)
   pause(delay/2)
   plot(x,y,"tracetype","lines","realstyle",0)
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","opensquare")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","square")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","opencircle")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","circle")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","opentriangle")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","triangle")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","openinvtriangle")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","invtriangle")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","opendiamond")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","diamond")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","cross")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","plus")
   pause(delay/2)
   plot(x,y,"tracetype","lines","symbolshape","none")
   pause(delay/2)
   plot(x,y,"tracecolor",[255,0,0])
   pause(delay/2)
   plot(x,y,"tracecolor",[255,255,0])
   pause(delay/2)
   plot(x,y,"tracecolor",[0,255,0])
   pause(delay/2)
   plot(x,y,"tracecolor",[0,255,255])
   pause(delay/2)
   plot(x,y,"tracecolor",[0,0,255])
   pause(delay/2)
   plot(x,y,"tracecolor",[255,0,255])
   pause(delay/2)
   plot(x,y,"tracecolor",[255,255,255])
   pause(delay/2)
   plot(x,y,"tracecolor",[255,255,0])
   for(w = 20 to 1 step -1)
      pause(delay/4)
      plot(x,y,"tracewidth",w)
   next(w)

endproc(1,"")