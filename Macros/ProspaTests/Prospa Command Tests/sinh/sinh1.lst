# scalars
sinh(0) = 0
sinh(1) = 1/2*(exp(1)-exp(-1))
sinh(2) = 1/2*(exp(2)-exp(-2))
sinh(10) = 1/2*(exp(10)-exp(-10))
# 1D matrix
sinh([0,1,2,10]) = [1/2*(exp(0)-exp(-0)),1/2*(exp(1)-exp(-1)),1/2*(exp(2)-exp(-2)),1/2*(exp(10)-exp(-10))]
# 2D matrix
sinh([0,1;2,10]) = [1/2*(exp(0)-exp(-0)),1/2*(exp(1)-exp(-1));1/2*(exp(2)-exp(-2)),1/2*(exp(10)-exp(-10))]
