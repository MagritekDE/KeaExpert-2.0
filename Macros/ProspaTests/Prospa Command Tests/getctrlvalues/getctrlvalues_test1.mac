procedure(getctrlvalues_test1,verbose,delay)

# Make a window and show it
   n = :windowdefinition()
   showwindow(n)

# Extract the control values
   lst = getctrlvalues(n)
   assignlist(lst)

# Text that all returned control values are as expected
   if(textmenu1 != "option2")
      return(0, "Invalid result for: getctrlvalues() textmenu")
   endif

   if(textmenu2 != 3)
      return(0, "Invalid result for: getctrlvalues() textmenu")
   endif

   if(textbox1 != "string")
      return(0, "Invalid result for: getctrlvalues() textbox string")
   endif

   if(textbox2 != 100)
      return(0, "Invalid result for: getctrlvalues() textbox number")
   endif

   if(slider1 != 50)
      return(0, "Invalid result for: getctrlvalues() slider position")
   endif

   if(rbutton1 != "b")
      return(0, "Invalid result for: getctrlvalues() radiobutton text")
   endif

   if(checkbox1 != "yes")
      return(0, "Invalid result for: getctrlvalues() checkbox text")
   endif

   if(colorbox1 != [0,255,0])
      return(0, "Invalid result for: getctrlvalues() colorbox value")
   endif

   if(stext1 != "some text")
      return(0, "Invalid result for: getctrlvalues() statitext string")
   endif

   if(stext2 != 42)
      return(0, "Invalid result for: getctrlvalues() statictext number")
   endif

# Wait a moment and close the window
   pause(delay)

   closewindow(n)

endproc(1,"")

procedure(windowdefinition)

   # Automatically generated window definition procedure.
   # Any code added manually will be removed if layout modified interactively
   n = window("getctrlvalues test", -1, -1, 225, 157)

      # Define all controls with basic parameters
       colorbox(1, 27, 23, 22, 22, [255,0,0])
       checkbox(2, 36, 64, "no,yes", "no")
       radiobuttons(3, 81, 27, 20, "horizontal", "a,b,c", "a")
       slider(4, 31, 96, 100, 20, "horizontal")
       textbox(5, 150, 15, 40)
       textbox(6, 150, 40, 40)
       textmenu(7, 150, 65, 60, 200)
       textmenu(8, 150, 90, 60, 200)
       statictext(9, 81, 55, "left","some text")
       statictext(10, 81, 70, "left","42")

     # Set other control parameters
       setpar(n,1,"color",[0,255,0])
       setpar(n,2,"text","yes")
       setpar(n,3,"text","b")
       setpar(n,4,"value",50)
       setpar(n,5,"text","string")
       setpar(n,6,"text","100")
       setpar(n,7,"menu",["option1","option2","option3"])
       setpar(n,8,"menu",["1","2","3"])
       setpar(n,7,"text","option2")
       setpar(n,8,"text","3")

       setpar(n,1,"name","colorbox1")
       setpar(n,2,"name","checkbox1")
       setpar(n,3,"name","rbutton1")
       setpar(n,4,"name","slider1")
       setpar(n,5,"name","textbox1")
       setpar(n,6,"name","textbox2")
       setpar(n,7,"name","textmenu1")
       setpar(n,8,"name","textmenu2")
       setpar(n,9,"name","stext1")
       setpar(n,10,"name","stext2")
      

endproc(n)