procedure(if_test1,verbose,delay)

  if(:test(1) != 1)
      return(0, "Invalid result for if_test #1")
   endif

   if(:test(2) != 2)
      return(0, "Invalid result for if_test #2")
   endif

   if(:test(3) != 3)
      return(0, "Invalid result for if_test #3")
   endif

   if(:test(4) != 4)
      return(0, "Invalid result for if_test #4")
   endif

endproc(1,"")

procedure(test,a)

   if (a == 1)
     return(1)
   elseif (a == 2)
     return(2)
   elseif (a == 3)
     return(3)
   else
     return(4)
   endif

endproc(1,"")