procedure(title_test1,verbose,delay)
 
   m = noise(1000)
   plot(m)
   title("Title test - some noise \G(s) = 1")
   pause(1)
   m = [0:1:100]^2
   plot(m)
   title("y = x^(2)")
   
   m = noise(100,100)
   image(m)
   title("Title test - some noise \G(s) = 1")
   pause(1)
   m = [-100:100]
   m = (m'*m)^2
   image(m)
   title("y = x^(2)")

endproc(1,"")
