

procedure(rmfromlist_test1,verbose,delay)

   a = ["a = 1", "b = 2", "c = 3", "d = 4"]

   if(rmfromlist(a,1) != ["a = 1","c = 3","d = 4"])
     return(0, "Invalid result for: rmfromlist_test1 #1")
   endif 

   if(rmfromlist(a,"b") != ["a = 1","c = 3","d = 4"])
     return(0, "Invalid result for: rmfromlist_test1 #1")
   endif 
  
   if(rmfromlist(a,"b = 2") != ["a = 1","c = 3","d = 4"])
     return(0, "Invalid result for: rmfromlist_test1 #1")
   endif 
  

endproc(1,"")

