# scalars
tanh(0) = 0
tanh(1) = (exp(1)-exp(-1))/(exp(1)+exp(-1))
tanh(2) = (exp(2)-exp(-2))/(exp(2)+exp(-2))
tanh(10) = (exp(10)-exp(-10))/(exp(10)+exp(-10))
# 1D matrix
tanh([0,1,2,10]) = [0,(exp(1)-exp(-1))/(exp(1)+exp(-1)),(exp(2)-exp(-2))/(exp(2)+exp(-2)),(exp(10)-exp(-10))/(exp(10)+exp(-10))]
# 2D matrix
tanh([0,1;2,10]) = [0,(exp(1)-exp(-1))/(exp(1)+exp(-1));(exp(2)-exp(-2))/(exp(2)+exp(-2)),(exp(10)-exp(-10))/(exp(10)+exp(-10))]
