procedure(getcolormap_test1,verbose,delay)

   m = matrix(100,100)
   r = 30
   x = [0:(2*r)]
   y = sqrt(r^2-(x-r)^2)
   yf = matrix(100)
   yf = insert(yf,y,20)
   image(yf'*yf)
   cmap(rainbow)
   showcmap("yes")
   map = getcolormap()
   if(map != rainbow)
      return(0, "Invalid result for: getcolormap_test1()")
   endif


endproc(1,"")