# real scalars
trunc(0) = 0
trunc(1.0) = 1
trunc(1.45) = 1
trunc(1.5) = 1
trunc(1.95) = 1
trunc(2.01) = 2
trunc(-1.1) = -1
trunc(-1.45) = -1
trunc(-1.5) = -1
trunc(-1.95) = -1
trunc(-2.01) = -2
# complex scalars
trunc(1.1+1.1i) = 1+i
trunc(1.45+1.45i) = 1+i
trunc(1.5+1.5i) = 1+1i
trunc(1.95+1.95i) = 1+1i
trunc(2.01+2.01i) = 2+2i
trunc(-1.1-1.1i) = -1-1i
trunc(-1.45-1.45i) = -1-1i
trunc(-1.5-1.5i) = -1-1i
trunc(-1.95-1.95i) = -1-1i
trunc(-2.01-2.01i) = -2-2i
# real 1D matrix
trunc([0,1.1,1.45,1.5,1.95,2.01]) = [0,1,1,1,1,2]
trunc([-1.1,-1.45,-1.5,-1.95,-2.01]) = [-1,-1,-1,-1,-2]
# real 2D matrix
trunc([1.1,1.45;1.5,1.95]) = [1,1;1,1]
trunc([-1.1,-1.45;-1.5,-1.95]) = [-1,-1;-1,-1]
# complex 1D matrix
trunc({1.1+1.1i,1.45+1.45i,1.5+1.5i,1.95+1.95i}) = {1+i,1+i,1+1i,1+1i}
trunc({-1.1-1.1i,-1.45-1.45i,-1.5-1.5i,-1.95-1.95i}) = {-1-i,-1-i,-1-1i,-1-1i}
# complex 2D matrix
trunc({1.1+1.1i,1.45+1.45i;1.5+1.5i,1.95+1.95i}) = {1+i,1+i;1+1i,1+1i}
trunc({-1.1-1.1i,-1.45-1.45i;-1.5-1.5i,-1.95-1.95i}) = {-1-i,-1-i;-1-1i,-1-1i}
