# scalars
abs(1) = 1
abs(-1) = 1
abs(0) = 0
abs(i) == 1
abs(1+i) = sqrt(2)
abs(1-i) = sqrt(2)
abs(-1+i) = sqrt(2)
abs(1-i) = sqrt(2)
# 1D matrix
abs([1,-2,4,-6]) = [1,2,4,6]
abs({i,1,i+1,i-1}) = [1,1,sqrt(2),sqrt(2)]
# 2D matrix
abs([1,-2;4,-6]) = [1,2;4,6]
abs({i,1;i+1,i-1}) = [1,1;sqrt(2),sqrt(2)]
