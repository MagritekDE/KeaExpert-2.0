procedure(vectorplot_test1,verbose,delay)
 
   mx = matrix(100,100)
   my = matrix(100,100)

   vx = [0:1:99]
   vy = sin(vx*2*pi/100)
   mx = vy'*vy
   vy = cos(vx*2*pi/100)
   my = vy'*vy
   
   draw2d("false")
      vectorplot(mx,my,20,5,5)
      title("Vortices")
      xlabel("x axis")
      ylabel("y axis")
   draw2d("true")

endproc(1,"")
