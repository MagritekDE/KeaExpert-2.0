# Plot factorial function
procedure(factorial_test2,verbose,delay)

   clear1d()
   x = [0:20]
   y = matrix(size(x))
   for(k = 0 to 20)
      y[k] = factorial(k)
   next(k)
   plot(x,y)
   axes("ymapping","log")
   title("factorial function")
   ylabel("factorial(x)")
   pause(delay)

endproc(1,"")
