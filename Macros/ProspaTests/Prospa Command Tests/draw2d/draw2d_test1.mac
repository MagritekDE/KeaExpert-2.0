procedure(draw2d_test1,verbose,delay)

clear2d()
t = [0:1:100]
curplot("2d")

for(n = 0 to 100)

  draw2d("false")
  y = sin(5*t/(10+n))'*cos((5*t/(10+n)))
  image(y)
  title("sin(5*t/($10+n$))'*cos((5*t/($10+n$)))")
  xlabel("x label for n = $n$")
  ylabel("y label for n = $n$")
  draw2d("true")

next(n)
   
endproc(1,"")
