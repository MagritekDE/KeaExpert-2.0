procedure(windowmargins_test1,verbose,delay)

  curplot("1d")
  (x1,x2,y1,y2) = windowmargins("getargs")
  for(k = 10 to 100)
     windowmargins(k,k,k,k)
     pause(delay)
  next(k)
  windowmargins(x1,x2,y1,y2)

  curplot("2d")
  (x1,x2,y1,y2) = windowmargins("getargs")
  for(k = 10 to 100)
     windowmargins(k,k,k,k)
     pause(delay)
  next(k)
  windowmargins(x1,x2,y1,y2)
   
endproc(1,"")
