procedure(contour_test1,verbose,delay)
 
   clear2d
   x = [0:100]/100*2*pi
   y = sin(x)
   draw2d("false")
   image(y'*y)
   title("intensity plot")
   contour(10,1)
   cmap(spectrum)
   draw2d("true")
   pause(delay)
   draw2d("false")
   image(y'*y)
   title("contour plot")
   contour(10,2)
   draw2d("true")
   pause(delay)
   draw2d("false")
   image(y'*y)
   title("interpolated contour plot")
   contour(5,4)
   cmap(spectrum)
   draw2d("true")

endproc(1,"")
