procedure(textbox_test1,verbose,delay)

   n = window("textbox test",100,100,150,100)
   textbox(1,20,20,80)
   showwindow(n)

   for(k = 0 to 100)
     setpar(n,1,"text","n = $k$")
     pause(0.02)
   next(k)

   setpar(n,1,"password","on")
   setpar(n,1,"text","password")

   pause(delay)

   closewindow(n)

endproc(1,"")
