
procedure(axispar3d_test1,verbose,delay)

for(m = 0.1 to 1 step 0.1)
   clear3d
   draw3d("false")
   axispar3d(m,1,1,2)
   color3d([1,1,0])
   axis3d("x",[0,20],0,0,[0,1],"x")
   axis3d("y",[0,20],0,0,[0,2],"y")
   axis3d("z",[0,20],0,0,[0,5],"z")
   viewdistance(70)
   rotate3d(-18,326,0)
   draw3d("true")
   pause(delay)
next(m)

