procedure(realtostr_test1,verbose,delay)

   a = 12.345
   if(realtostr(a) != "12.345")
      return(0, "Invalid result for: realtostr_test1 #1")
   endif  

   a = -12.345
   if(realtostr(a) != "-12.345")
      return(0, "Invalid result for: realtostr_test1 #2")
   endif 

   a = 12.23e11
   if(realtostr(a) != "1.223e+012")
      return(0, "Invalid result for: realtostr_test1 #3")
   endif  

   a = 12.23e-11
   if(realtostr(a) != "1.223e-010")
      return(0, "Invalid result for: realtostr_test1 #4")
   endif  

endproc(1,"")
