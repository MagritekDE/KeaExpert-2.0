procedure(xlabel_test1,verbose,delay)
 
   clear1d()
   m = noise(1000)
   m = [0:1:100]^2
   plot(m)
   title("xlabel test")
   xlabel("Noise amplitude_(2) \G(s) = 1^(2)")
   pause(1)

   clear2d()
   m = noise(100,100)
   image(m)
   m = [-100:100]
   m = (m'*m)^2
   image(m)
   title("Title test")
   xlabel("Noise amplitude_(2) \G(s) = 1^(2)")
   pause(1)

endproc(1,"")