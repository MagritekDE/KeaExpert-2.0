procedure(cmatrix_test1,verbose,delay)
 
   clear3d()
   m = cmatrix(10)
   pr m
   pr "\n   variable type of m: $vartype(m)$\n"
   pr "   dimensions of m : $size(m)$\n"
   pause(delay)
   m = cmatrix(3,5)
   pr m
   pr "\n   variable type of m: $vartype(m)$\n"
   pr "   dimensions of m : $size(m)$\n"
   pause(delay)
   m = cmatrix(3,5,3)
   pr m
   pr "\n   variable type of m: $vartype(m)$\n"
   pr "   dimensions of m : $size(m)$\n"
   pause(delay)

endproc(1,"")
